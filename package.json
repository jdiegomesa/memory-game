{
  "name": "memory_game",
  "version": "0.1.0",
  "description": "Technical test for Modyo",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "13.5.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-scripts": "5.0.1",
    "web-vitals": "2.1.4"
  },
  "scripts": {
    "build:css": "postcss src/styles/main.css -o src/index.css",
    "watch:css": "postcss src/styles/main.css -o src/index.css -w",
    "start": "npm-run-all -p watch:css start-js",
    "start-js": "react-scripts start",
    "build-js": "react-scripts build",
    "build": "npm-run-all build:css build-js",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jdiegomesa/memory-game.git"
  },
  "keywords": [
    "reactjs",
    "tailwindcss",
    "frontend",
    "postcss"
  ],
  "author": "Juan Diego Mesa O",
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "0.4.2",
    "@tailwindcss/nesting": "0.0.0-insiders.565cd3e",
    "autoprefixer": "10.4.14",
    "cssnano": "6.0.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.23",
    "postcss-cli": "10.1.0",
    "postcss-custom-media": "9.1.3",
    "postcss-import": "15.1.0",
    "postcss-nested": "6.0.1",
    "postcss-sort-media-queries": "5.1.0",
    "tailwindcss": "3.3.2"
  }
}
